{
    "name": "jdlabs/nova-metrics",
    "description": "custom laravel-nova metrics",
    "keywords": [
        "laravel",
        "nova",
        "metrics",
        "filters"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Jdlabs\\NovaMetrics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jdlabs\\NovaMetrics\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
